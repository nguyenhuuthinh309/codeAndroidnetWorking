    <label for="">SĐT</label>
     <input value="{{result.sdt}}" class="form-control" type="sdt" name="sdt" placeholder="sdt">
</div>
 <div class="form-group">
    <label for="">Ngày Thuê</label>
     <input value="{{result.ngaythue}}" class="form-control" type="ngaythue" name="ngaythue" placeholder="ngaythue">
</div>
 <div class="form-group">
    <label for="">Khung Giờ</label>
     <input value="{{result.khunggio}}" class="form-control" type="khunggio" name="khunggio" placeholder="khunggio">
</div>
 <div class="form-group">
    <label for="">Tổng Tiền </label>
     <input value="{{result.tongtien}}" class="form-control" type="tongtien" name="tongtien" placeholder="tongtien">
</div>
 <div class="form-group">
    <label for="">Trạng Thái</label>
     <input value="{{result.trangthai}}" class="form-control" type="trangthai" name="trangthai" placeholder="trangthai">
</div>
<br>
<br>
 <br><br>
 <div class="form-group">
    <button type="submit" class="btn btn-info">submit</button>
        <a href="/product/list" class="btn btn-primary">view tài khoản</a>
 </div>
</form>
