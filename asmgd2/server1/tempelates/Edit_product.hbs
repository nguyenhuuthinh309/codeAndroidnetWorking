     <h3>{{title1}}</h3>


<form action="/product/update" method="post" autocomplete="off">

<div class="form-group">
    <label for="">Id</label>
    <input type="text" name="id" class="form-control" placeholder="id" value="{{result.id}}">
    </div>
<div class="form-group">
    <label for="">Tên Khách</label>
    <input type="text" name="tenkhach" class="form-control" placeholder="tenkhach" value="{{result.tenkhach}}">
    </div>
